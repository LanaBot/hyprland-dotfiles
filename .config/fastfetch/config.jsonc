{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
    	"source": "~/.config/fastfetch/ghost.txt",
    	"type": "file-raw"
    },
    "display": {
        "separator": " "
    },
    "modules": [
        {
            "key": "          ",
            "type": "custom"
        },
        {
            "key": "╭───────────╮",
            "type": "custom"
        },
        {
            "key": "│ {#31} user    {#keys}│",
            "type": "title",
            "format": "{user-name}"
        },
        // {
        //     "key": "│ {#32}󰇅 hname   {#keys}│",
        //     "type": "title",
        //     "format": "{host-name}"
        // },
        {
            "key": "│ {#32}󰅐 uptime  {#keys}│",
            "type": "uptime"
        },
     
        {
            "key": "│ {#33} kernel  {#keys}│",
            "type": "kernel"
        },
        {
            "key": "│ {#34}󰇄 desktop {#keys}│",
            "type": "de"
        },
        {
            "key": "│ {#35} term    {#keys}│",
            "type": "terminal"
        },
        {
            "key": "│ {#36} shell   {#keys}│",
            "type": "shell"
        },
        {
            "key": "│ {#31}󰍛 cpu     {#keys}│",
            "type": "cpu",
            "showPeCoreCount": true
        },        {
            "key": "│ {#32}󰍛 gpu     {#keys}│",
            "type": "gpu"
        },
        {
            "key": "│ {#33}󰉉 disk    {#keys}│",
            "type": "disk",
            "folders": "/"
        },
        {
            "key": "│ {#34} memory  {#keys}│",
            "type": "memory"
        },
        {
            "key": "│ {#35}󰩟 network {#keys}│",
            "type": "localip",
            "format": "{ipv4} ({ifname})"
        },
        {
            "key": "├───────────┤",
            "type": "custom"
        },
        {
            "key": "│ {#39} colors  {#keys}│",
            "type": "colors",
            "symbol": "circle"
        },
        {
            "key": "╰───────────╯",
            "type": "custom"
        }
    ]
}
